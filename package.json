{
	"name": "postman-schema-editor",
	"displayName": "Postman Schema Editor",
	"description": "Edit your Postman Schema directly into VS Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:postman-schema-editor.fetchPostmanSchema"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "postman-schema-editor.fetchPostmanSchema",
				"title": "Fetch Postman Schema"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"async": "^3.2.0",
		"lodash": "^4.17.20",
		"request": "2.88.2"
	}
}
